[
    (def-type :nabs [
        n
        a.
        b^
        s$
    ])

    (set :nabs._new^ (do_ [:nabs:self] [
        (print ":nabs._new^" 'nl$)
        (set :nabs:self.n 3)
        (set :nabs:self.a. (new ':number-array [5]))
        (set :nabs:self.s$ (new ':string [6]))
        (str-copy$ :nabs:self.s$ "hello")
        (set :nabs:self.b^ 'false)
      ] 'true)
    )

    (set :nabs._del^ (do_ [:nabs:this] [
        (print ":nabs._del^" 'nl$)
        (del :nabs:this.a.)
        (del :nabs:this.s$)
      ] 'true)
    )
]
