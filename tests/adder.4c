(set main (do [] ':int [
    (set add2 (do [x ':int] ':int [
        (return (+ 3 x))
    ]))

    (print (add2 3) 'nl)

    (set make-adder (do [x ':int] ':func [
        (return (do [y ':int] ':int [
            (return (+ x y))
        ]))
    ]))

    (set add4 (make-adder 4))
    (print (add4 3) 'nl)

    (print (eval (make-adder 5) 2) 'nl)
    (return 0)
]))

