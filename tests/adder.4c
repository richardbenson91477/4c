
(set main (do [] ':number [
    (set add2 (do [x] ':number [
        (return (+ 3 x))
    ]))

    (print (add2 3) 'nl)

    (set make-adder (do [x] ':function [
        (return (do [y] ':number [
            (return (+ x y))
        ]))
    ]))

    (set add4 (make-adder 4))
    (print (add4 3) 'nl)

    (print ((make-adder 5) 2) 'nl)
  ] 0)
)

