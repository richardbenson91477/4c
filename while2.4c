
(def main (do [] [
    (set i 4)
    (while (> i 0) [
        (print (str-from (char-from i) 'spc 'nil))

        (set j 4)
        (while (> j 0) [
            (print (str-from (char-from (+ j 64)) 'nil))
            ] [(set j (- j 1))]
        )

        (print 'nl)
      ] [(set i (- i 1))]
    )

    (print 'nl)
  ] 0)
)

