
(def-type :nabs [
    n
    a.
    b^
    s$
])

(def :nabs.new (do [:nabs:self] [
    (print ":nabs.new")
    (set :nabs:self.n 3)
    (set :nabs:self.a. (new 'number [5]))
    (set :nabs:self.s$ (new 'string [6]))
    (str-copy :nabs:self.s$ "hello")
    (set :nabs:self.b^ 'false)
    ] 'true)
)

(def :nabs.del (do [:nabs:this] [
    (del :nabs:this.a.)
    (del :nabs:this.s$)
  ] 'true)
)

