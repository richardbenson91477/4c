(load "../view")

(def :nabs (is
    n
    a.
    b^
    s$
))

(def :nabs.new^ (do
    (print$ ":nabs.new^")
    (set n 3)
    (set a. (.new% 5))
    ; ? (copy a. 5 4 3 2 1)
    (set s$ ($new% 6))
    (str-copy s$ "hello\0")
    (set b^ false)
  true)
)

(def :nabs.del^ (do
    (^del. a.)
    (^del$ s$)
  true)
)

