
(set-func main (do [] #:int [
    (set-i i 4)
    (while (gt-i i 0) [
        (print-s (str-from-char (add-i i 96)))

        (set j 4)
        (while (gt-i j 0) [
            (print-s (str-from-char (add-i j 64)))
            ] 
          [(set-i j (sub-i j 1))]
        )

        (print-s 'nl)
      ] [(set-i i (sub-i i 1))]
    )

    (print-s 'nl)
    (return-i 0)
]))

