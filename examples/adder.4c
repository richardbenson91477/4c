(set-func main (do [] #:int [
    (set-func add2 (do [x #:int] #:int [
        (return-i (add-i 3 x))
    ]))

    (print-i (add2 3) 'nl)

    (set make-adder (do [x #:int] #:func [
        (return-func (do [y #:int] #:int [
            (return-i (add-i x y))
        ]))
    ]))

    (set-func add4 (make-adder 4))
    (print-i (add4 3) 'nl)

    (print-i (eval (make-adder 5) 2))
    (print-s 'nl)
    (return 0)
]))

